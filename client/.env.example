# API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# App Name
REACT_APP_NAME=Toosila

# Environment
NODE_ENV=development

# Error Tracking and Monitoring

# Sentry Configuration (Frontend)
# Sign up at: https://sentry.io/signup/
# Create a React project (separate from backend) and copy your DSN here
# If not set, Sentry error tracking will be disabled
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/your-react-project-id
REACT_APP_SENTRY_ENVIRONMENT=development

# Optional: Performance Monitoring
# Set to 1.0 (100%) for development, 0.1 (10%) for production
REACT_APP_SENTRY_TRACES_SAMPLE_RATE=1.0
