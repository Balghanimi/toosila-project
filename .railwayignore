client/build
client/node_modules
server/node_modules
node_modules

# Ignore development files
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore test files
client/tests
client/test-results
client/playwright-report

# Ignore backup files
client/backup-*
